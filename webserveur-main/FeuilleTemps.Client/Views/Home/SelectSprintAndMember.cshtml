@model FeuilleTemps.Web.Models.SelectSprintAndMemberViewModel

<h2>Select Sprint and Member</h2>

<form method="post">
    <div>
        <label for="sprint">Sprint:</label>
        <select id="sprint" name="sprint">
            @foreach (var sprint in Model.Sprints)
            {
                <option value="@sprint.Id">@sprint.Nom</option>
            }
        </select>
    </div>
    <div>
        <label for="member">Member:</label>
        <select id="member" name="member">
            @foreach (var member in Model.Members)
            {
                <option value="@member.Id">@member.Nom</option>
            }
        </select>
    </div>
    <button type="submit">Submit</button>
</form>
